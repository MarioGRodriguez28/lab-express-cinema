<div>

  <nav>
    <a href="/">
      <button>Home</button>
    </a>
    <a href="/movies">
      <button>Movies</button>
    </a>
  </nav>
  <br />
  <h1>Ironhack Cinema</h1>
  <h3>Click on the movie to check the showtimes!</h3>
  <div id="container">
    {{#each allMovies}}

      <img src="{{this.image}}" alt="Imagenes de peliculas" />
      
        <h2>{{this.title}} </h2>
        <p>
          <a href="./movies/{{this.id}}">See more</a>
        </p>
      

    {{/each}}
  </div>
</div>